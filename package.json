    "node-pg": "^1.0.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.0-rc.4",
    "sequelize": "^4.22.1",
    "sequelize-cli": "^3.0.0",
    "serve-favicon": "~2.4.5",
    "bower": "0.6.x"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
